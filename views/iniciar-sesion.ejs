<main class="contenedor contenedor-formularios">
    <h1><%=nombrePagina%></h1>
    <form action="/iniciar-sesion" method="POST" class="default-form">
      <div class="campo">
        <label for="">Email</label>
        <input type="email" name="email" placeholder="Tu Email" value="p@p.com"/>
      </div>
      
      <div class="campo">
        <label for="">Password</label>
        <input type="password" name="password" placeholder="Tu password" value="p"/>
      </div>
      
      <div class="campo enviar">
        <input type="submit" value="Iniciar Sesion" class="btn btn-rosa" />
      </div>
    </form>
  </main>
  